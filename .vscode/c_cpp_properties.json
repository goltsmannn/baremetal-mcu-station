{
    "configurations": [
        {
            "name": "Mac",
            "includePath": [
                "${workspaceFolder}/**"
            ],
            "defines": [],
            "compilerPath": "/usr/bin/clang",
            "cStandard": "c17",
            "cppStandard": "c++17",
            "intelliSenseMode": "macos-clang-arm64"
        },
        {
            "name": "Arduino-UNO",
            "includePath": [
                "${workspaceFolder}/lib/**",
                "${workspaceFolder}/include",       
                "${workspaceFolder}/src",
                "${workspaceFolder}/.pio/libdeps/uno/**",
                "${workspaceFolder}/.pio/build/uno",
                "${userHome}/.platformio/packages/toolchain-atmelavr/avr/include",
                "${userHome}/.platformio/packages/toolchain-atmelavr/lib/gcc/avr/*/include"
            ],
            "defines": [
                "ARDUINO=10819",
                "ARDUINO_AVR_UNO",
                "F_CPU=16000000L",
                "__AVR_ATmega328P__"
            ],
            "compilerPath": "${userHome}/.platformio/packages/toolchain-atmelavr/bin/avr-gcc",
            "cStandard": "c11",
            "cppStandard": "c++17",
            "intelliSenseMode": "gcc-x64"
        }
    ],
    "version": 4
